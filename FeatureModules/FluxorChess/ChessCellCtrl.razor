
<div>
    
@if (ChessPiece != null)
{
        <img alt="@CellId" src="@ChessPiece?.ImageSrc" />
}
else
{
    <img alt="@CellId" src="Images/blank.svg" />
}
    

</div>

    



@code {

    [Parameter]
    public ChessPiece? ChessPiece { get; set; } 

    [Parameter]
    public string? CellId { get; set; }
}
